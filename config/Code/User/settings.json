{
    /**
    * Breadcrumps
    */
    "breadcrumbs.enabled": false,

    /**
    * Code Runner
    */
    "code-runner.clearPreviousOutput": true,
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.defaultLanguage": "fish",

    /**
    * Editor
    */
    "editor.fontFamily": "'JetBrains Mono','FontAwesome','M+ 1m'",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.accessibilitySupport": "off",
    "editor.formatOnType": false,
    "editor.codeLens": true,
    "editor.colorDecorators": true,
    "editor.cursorStyle": "line",
    "editor.cursorSurroundingLines": 3,
    "editor.cursorWidth": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.folding": true,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.glyphMargin": true,
    "editor.lineHeight": 25,
    "editor.lineNumbers": "on",
    "editor.scrollBeyondLastLine": true,
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.tabSize": 4,
    "editor.renderWhitespace": "all",
    "editor.useTabStops": true,
    "editor.wordWrapColumn": 80,
    "editor.wordWrap": "off",
    "editor.rulers": [80],

    /**
    * Explorer
    */
    "explorer.compactFolders": true,
    "explorer.openEditors.visible": 0,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,

    /**
    * Favorites
    */
    "favorites.storageRegistry": [
        ".vscode/favorites/ansible.json",
        ".vscode/favorites/backups.json",
        ".vscode/favorites/desktops.json",
        ".vscode/favorites/dotfiles.json",
        ".vscode/favorites/dropbox.json",
        ".vscode/favorites/fish.json",
        ".vscode/favorites/links.json",
        ".vscode/favorites/repos.json"
    ],

    /**
    * File Settings
    */
    "files.autoSave": "off",
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "USE_GITIGNORE": true
    },
    "files.associations": {
        "all": "yaml",
        "dunstrc": "ini",
        "config": "i3",
        "*.base": "ini",
        "polybar": "ini",
        "*.dconf": "ini",
        "*.conf": "xml"
    },

    "[jsonc]": {
        "files.encoding": "utf8",
        "editor.tabSize": 4,
        "editor.defaultFormatter": "vscode.json-language-features"
    },

    "[fish]": {
        "files.encoding": "utf8",
        "editor.tabSize": 4
    },

    "[yaml]": {
        "files.encoding": "utf8",
        "editor.tabSize": 4,
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },

    /**
     * Projects
     */
    "projectTemplates.templatesDirectory": ".vscode/templates",

    /**
    * Searching
    */
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/coverage": true,
        "**/dist": true,
        "**/build": true
    },

    /**
    * Telementry
    */
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,

    /**
    * Terminals
    */
    "terminal.integrated.fontFamily": "Iosevka Term SS04",
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "terminal.integrated.shellArgs.linux": ["--login"],

    /**
    * Windows
    */
    "window.zoomLevel": 1,

    /**
    * Workbench
    */
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.settings.editor": "json",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#2F3238"
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Firefox Dark",
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.closeOnFileDelete": true,
    "GithubGistExplorer.explorer.gistSortBy": "Label",
    "GithubGistExplorer.explorer.gistAscending": true,
    "GithubGistExplorer.explorer.subscriptionSortBy": "Last Updated",
    "GithubGistExplorer.explorer.subscriptionAscending": false,
    "GithubGistExplorer.github.username": "cbdowell",
    "window.titleBarStyle": "custom",
    "window.menuBarVisibility": "compact",
    "workbench.tips.enabled": false,
    "sync.autoDownload": true,
    "sync.quietSync": true,
    "sync.autoUpload": true,
    "sync.gist": "4d221083a73888aa48f36363879a0d31",
    "sync.forceUpload": true,
    "GithubGistExplorer.github.token": "aa898361dbc499d08041332ac9888d589c2fe479"
}
